  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "BcryptGen",
      "request": "launch",
      "mainClass": "it.serravalle.cameras_api.utils.BcryptGen",
      "projectName": "cameras-api"
    },
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "CamerasApiApplication",
      "request": "launch",
      "mainClass": "it.serravalle.cameras_api.CamerasApiApplication",
      "projectName": "cameras-api",
      "vmArgs": "-Djavax.net.ssl.trustStore=C:\\certs\\truststore\\truststore.jks -Djavax.net.ssl.trustStorePassword=changeit -Djavax.net.ssl.trustStoreType=JKS"
    },
    {
      "type": "java",
      "name": "CamerasApplication",
      "request": "launch",
      "mainClass": "it.serravalle.cameras.CamerasApplication",
      "projectName": "cameras",
      "vmArgs": "-Djavax.net.ssl.trustStore=C:\\certs\\truststore\\truststore.jks -Djavax.net.ssl.trustStorePassword=changeit -Djavax.net.ssl.trustStoreType=JKS"
    }
  ]
}
